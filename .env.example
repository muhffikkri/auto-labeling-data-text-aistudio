GOOGLE_API_KEY_1="MASUKKAN_API_KEY_PERTAMA_ANDA_DI_SINI"# Skrip akan otomatis mendeteksi GOOGLE_API_KEY_1, GOOGLE_API_KEY_2, dst.

GOOGLE_API_KEY_1="MASUKKAN_API_KEY_PERTAMA_ANDA_DI_SINI"

# API key tambahan (opsional - untuk load balancing dan quota management)GOOGLE_API_KEY_2="MASUKKAN_API_KEY_KEDUA_ANDA_DI_SINI"
GOOGLE_API_KEY_2="MASUKKAN_API_KEY_KEDUA_ANDA_DI_SINI"
GOOGLE_API_KEY_3="MASUKKAN_API_KEY_KETIGA_ANDA_DI_SINI"

# Catatan: Tambahkan sebanyak mungkin API keys (GOOGLE_API_KEY_4, dst.)
