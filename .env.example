# ===== KONFIGURASI DASAR =====
# Salin file ini menjadi .env dan isi dengan nilai yang sesuai

# Model fallback list - urutan dari kualitas/biaya tertinggi ke terendah
# Format: comma-separated list tanpa spasi ekstra
MODEL_FALLBACK_LIST="gemini-2.5-pro,gemini-2.5-flash,gemini-2.5-flash-lite,gemini-2.0-flash,gemini-2.0-flash-lite"

# Model utama (akan otomatis diset ke model pertama dalam fallback list)
MODEL_NAME="gemini-2.5-pro"

# Direktori output dan dataset
OUTPUT_DIR="results"
DATASET_DIR="datasets"

# ===== API KEYS (Multiple keys untuk auto-rotation) =====
# API key utama (wajib diisi)
GOOGLE_API_KEY_1="MASUKKAN_API_KEY_PERTAMA_ANDA_DI_SINI"

# API key tambahan (opsional - untuk load balancing dan quota management)
GOOGLE_API_KEY_2="MASUKKAN_API_KEY_KEDUA_ANDA_DI_SINI"
GOOGLE_API_KEY_3="MASUKKAN_API_KEY_KETIGA_ANDA_DI_SINI"
GOOGLE_API_KEY_4="MASUKKAN_API_KEY_KEEMPAT_ANDA_DI_SINI"
GOOGLE_API_KEY_5="MASUKKAN_API_KEY_KELIMA_ANDA_DI_SINI"

# Catatan: Tambahkan sebanyak mungkin API keys (GOOGLE_API_KEY_6, dst.)
# Sistem akan otomatis rotasi semua keys yang tersedia

# ===== PENGATURAN PROCESSING =====
MAX_RETRIES=3
TIMEOUT_SECONDS=30
REQUEST_DELAY=1.0

# ===== LOGGING & MONITORING =====
LOG_LEVEL="INFO"                    # DEBUG, INFO, WARNING, ERROR
ENABLE_REQUEST_TRACKING=true        # Track API requests dan statistik
ENABLE_SESSION_LOGGING=true         # Log sessions untuk analisis

# ===== SINGLE FILE OUTPUT SYSTEM =====
# Aktifkan single file output dengan resume capability (direkomendasikan: true)
SINGLE_FILE_OUTPUT=true
ENABLE_RESUME_CAPABILITY=true

# ===== OPTIMASI BATCH =====
# Ukuran batch default (bisa diubah di GUI)
DEFAULT_BATCH_SIZE=50
MAX_BATCH_SIZE=300

# Skip optimization untuk efisiensi quota
SKIP_COMPLETED_BATCHES=true
SKIP_PARTIAL_BATCHES=true

# ===== PENGATURAN GUI =====
# Interval update progress tracking (detik)
PROGRESS_UPDATE_INTERVAL=2.0

# Interval auto-save untuk persistensi progress (detik)
AUTO_SAVE_INTERVAL=30

# ===== ERROR HANDLING =====
# Auto-retry pada network errors
AUTO_RETRY_ON_NETWORK_ERROR=true

# Auto-fallback ke model berikutnya saat quota habis
AUTO_MODEL_FALLBACK=true

# Graceful shutdown saat interupsi
ENABLE_GRACEFUL_SHUTDOWN=true

# ===== PERFORMANCE TUNING =====
# Thread pool size untuk operasi concurrent
THREAD_POOL_SIZE=4

# Memory optimization untuk dataset besar
ENABLE_MEMORY_OPTIMIZATION=true

# Disk space management
AUTO_CLEANUP_OLD_LOGS=true
LOG_RETENTION_DAYS=30

# ===== CATATAN MODEL GEMINI =====
# Quota informasi (per Oktober 2025):
# gemini-2.5-pro: RPD=100 (kualitas tertinggi, quota terbatas)
# gemini-2.5-flash: RPD=250 (balance kualitas-speed)
# gemini-2.5-flash-lite: RPD=1000 (speed tinggi, quota besar)
# gemini-2.0-flash: RPD=200, TPM=1M (cocok untuk dokumen panjang)
# gemini-2.0-flash-lite: RPD=200, RPM=30 (speed tertinggi)
# 
# Model yang mungkin tidak accessible di beberapa akun:
# - gemini-2.5-flash-preview: Model not found
# - gemini-2.5-flash-lite-preview: Model not found